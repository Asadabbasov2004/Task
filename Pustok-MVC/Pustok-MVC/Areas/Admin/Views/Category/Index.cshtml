@model List<Catagory>
@{
    int count = 0;
}
<div class="container ">
    <div class="row">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">No#</th>
                <th scope="col">Id</th>
                <th scope="col">Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                count++;
                <tr>
                    <th scope="row">@count</th>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.CatagoryId</td>
                </tr>
            }
        </tbody>
    </table>
        <a class="btn btn-primary" asp-controller="Category" asp-action="Create">Add</a>
    </div>
    

</div>